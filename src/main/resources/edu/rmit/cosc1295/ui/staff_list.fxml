<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400" prefWidth="600"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.rmit.cosc1295.ui.StaffListController">

    <children>

        <TableView fx:id="staffTable" layoutX="50" layoutY="50" prefHeight="250" prefWidth="500">
            <columns>
                <TableColumn fx:id="idCol" prefWidth="120" text="ID" />
                <TableColumn fx:id="nameCol" prefWidth="200" text="Name" />
                <TableColumn fx:id="roleCol" prefWidth="160" text="Role" />
            </columns>

        </TableView>
        <Button fx:id="backBtn"
                layoutX="250" layoutY="330"
                onAction="#onBack" text="Back" />
    </children>
</AnchorPane>
